      <body>
         <lg>
            <l xml:id="l1"><anchor xml:id="l1a1"/>Se<anchor xml:id="l1a2"/> pronto non sarà <anchor xml:id="l1a3"/>l'uom<anchor xml:id="l1a4"/> <anchor xml:id="l1a5"/>al<anchor xml:id="l1a6"/> ben fare,</l>
            <l xml:id="l2"><anchor xml:id="l2a1"/>vedrassi 'n<anchor xml:id="l2a2"/> tempo <anchor xml:id="l2a3"/>e<anchor xml:id="l2a4"/> di virtù man<anchor xml:id="l2a5"/>ca<anchor xml:id="l2a6"/>re.</l>
            </lg>
         <lg>
            <l xml:id="l3">Per<anchor xml:id="l3a1"/>ché ·llo<anchor xml:id="l3a2"/> spa<anchor xml:id="l3a5"/>tio della vi<anchor xml:id="l3a6"/>t' <anchor xml:id="l3a3"/>è<anchor xml:id="l3a4"/> breve,</l>
            <l xml:id="l4"><anchor xml:id="l4a1"/>nes<anchor xml:id="l4a2"/>sun <anchor xml:id="l4a3"/>debbe<anchor xml:id="l4a4"/> <anchor xml:id="l4a5"/>tardare<anchor xml:id="l4a6"/> <anchor xml:id="l4a7"/>'l<anchor xml:id="l4a8"/> suo dovere,</l>
            <l xml:id="l5" n="5"><anchor xml:id="l5a1"/>né<anchor xml:id="l5a2"/> questo <anchor xml:id="l5a3"/>far<anchor xml:id="l5a4"/> <anchor xml:id="l5a5"/>[a]<anchor xml:id="l5a6"/> <anchor xml:id="l5a7"/>alcun<anchor xml:id="l5a8"/> <anchor xml:id="l5a9"/>debb'<anchor xml:id="l5a10"/> esser <anchor xml:id="l5a11"/>greve<anchor xml:id="l5a12"/>,</l>
            <l xml:id="l6"><anchor xml:id="l6a1"/>pensando<anchor xml:id="l6a2"/> <anchor xml:id="l6a3"/>'l fin<anchor xml:id="l6a4"/> <anchor xml:id="l6a5"/>ch'aspetta<anchor xml:id="l6a6"/> di vedere.</l>
            <l xml:id="l7">Se non, s'<anchor xml:id="l7a1"/>aspett'<anchor xml:id="l7a2"/> <anchor xml:id="l7a3"/>ancor<anchor xml:id="l7a4"/> molto dolere,</l>
            <l xml:id="l8"><anchor xml:id="l8a1"/>quando<anchor xml:id="l8a2"/> 'l passato non potrà tornare.</l>
         </lg>
