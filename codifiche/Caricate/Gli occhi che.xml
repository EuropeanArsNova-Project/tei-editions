         <lg>
            <l xml:id="l1">
               Gli <anchor xml:id="l1a1"/>occhi<anchor xml:id="l1a2"/> che <anchor xml:id="l1a3"/>’n<anchor xml:id="l1a4"/> prima tanto bel piacere
            </l>
            <l xml:id="l2">
               mi <anchor xml:id="l2a1"/>porson<anchor xml:id="l2a2"/> <anchor xml:id="l2a3"/>dentr’<anchor xml:id="l2a4"/> al core
            </l>
            <l xml:id="l3">
               <anchor xml:id="l3a1"/>non<anchor xml:id="l3a2"/> <anchor xml:id="l3a7"/>seguitand’<anchor xml:id="l3a8"/> <anchor xml:id="l3a3"/>Amor[ẹ]<anchor xml:id="l3a4"/> <span type="space"/> mi <anchor xml:id="l3a5"/>fan<anchor xml:id="l3a6"/> dolere.
            </l> 
         </lg>
         <lg>    
            <l xml:id="l4">
               <anchor xml:id="l4a1"/>Vana<anchor xml:id="l4a2"/> <anchor xml:id="l4a3"/>speranç’<anchor xml:id="l4a5"/>, Amor<anchor xml:id="l4a4"/>, che nel <anchor xml:id="l4a6"/>passato<anchor xml:id="l4a7"/>
            </l>
            <l xml:id="l5" n="5">
               <anchor xml:id="l5a3"/>tempo<anchor xml:id="l5a4"/> <anchor xml:id="l5a5"/>mostronmi<anchor xml:id="l5a6"/> <anchor xml:id="l5a1"/>con<anchor xml:id="l5a7"/> suo vaga vista<anchor xml:id="l5a2"/>,
            </l>
            <l xml:id="l6">
               <anchor xml:id="l6a1"/>m’à<anchor xml:id="l6a2"/> nelle <anchor xml:id="l6a3"/>forçe<anchor xml:id="l6a4"/> <anchor xml:id="l6a5"/>sue<anchor xml:id="l6a6"/> tanto legato
            </l>
            <l xml:id="l7">
               che gia <anchor xml:id="l7a5"/>ma’<anchor xml:id="l7a6"/> donna per suo <anchor xml:id="l7a1"/>non<anchor xml:id="l7a2"/> <anchor xml:id="l7a3"/>m’aquista<anchor xml:id="l7a4"/>.
            </l>
            <l xml:id="l8">
               <anchor xml:id="l8a1"/>Sempre<anchor xml:id="l8a2"/> piangendo l’anima s’atrista
            </l>
            <l xml:id="l9">
               <anchor xml:id="l9a3"/>e<anchor xml:id="l9a4"/> <anchor xml:id="l9a5"/>muorsi<anchor xml:id="l9a6"/> <anchor xml:id="l9a1"/>di<anchor xml:id="l9a2"/> dolore,
            </l>
            <l xml:id="l10" n="10">
               <anchor xml:id="l10a1"/>ché<anchor xml:id="l10a2"/> gli <anchor xml:id="l10a9"/>occhi<anchor xml:id="l10a10"/> <anchor xml:id="l10a3"/>ch’ànn’<anchor xml:id="l10a4"/> <anchor xml:id="l10a5"/>Amorẹ<anchor xml:id="l10a6"/> <span type="space"/> non <anchor xml:id="l10a7"/>può<anchor xml:id="l10a8"/> vedere.</l>
         </lg>
         <lg>    
            <l xml:id="l11">
               Così pien di tormento ’l cor mi veggio,
            </l>
            <l xml:id="l12">
               né so né penso che cuor <anchor xml:id="l12a1"/>l<supplied>[e]</supplied><anchor xml:id="l12a2"/> si mova.
            </l>
            <l xml:id="l13">
               Dimmi, signor, perché di male ’n peggio
            </l>
            <l xml:id="l14">
               costei mi guida non con giusta prova?
            </l>
            <l xml:id="l15" n="15">
               Ch’a gentil cor dé <anchor xml:id="l15a1"/>parer<anchor xml:id="l15a2"/> cosa nova
            </l>
            <l xml:id="l16">
               usando tal errore
            </l>
            <l xml:id="l17">
               verso di chi si <anchor xml:id="l17a1"/>muorẹ<anchor xml:id="l17a2"/> <span type="space"/> pe’ suo <anchor xml:id="l17a3"/>v<supplied>[o]</supplied>l<supplied>[e]</supplied>re<anchor xml:id="l17a4"/>.</l>
         </lg>
