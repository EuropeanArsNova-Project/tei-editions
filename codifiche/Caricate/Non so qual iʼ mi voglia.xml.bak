      <body>
         <lg>
            <l xml:id="l1">
                Non so qual <anchor xml:id="l1a1"/>iʼ<anchor xml:id="l1a2"/> mi voglia,
            </l>
            <l xml:id="l2">
                o <anchor xml:id="l2a1"/>viver<anchor xml:id="l2a2"/> o morir, per minor doglia.
            </l>
        </lg>
        <lg>
            <l xml:id="l3">
                Morir <anchor xml:id="l3a1"/>vorrei<anchor xml:id="l3a2"/>, ché ʼl viver mʼè gravoso
            </l>
            <l xml:id="l4">
                <anchor xml:id="l4a4"/>veggendomi<anchor xml:id="l4a5"/> per <anchor xml:id="l4a1"/>altrui <anchor xml:id="l4a2"/>esser<anchor xml:id="l4a3"/> lasciato;
            </l>
            <l xml:id="l5" n="5">
                <anchor xml:id="l5a1"/>e morte<anchor xml:id="l5a2"/> non <anchor xml:id="l5a3"/>vorrei<anchor xml:id="l5a4"/> ché, <anchor xml:id="l5a5"/>trapassato<anchor xml:id="l5a6"/>,
            </l>
            <l xml:id="l6">
                <anchor xml:id="l6a1"/>più<anchor xml:id="l6a2"/> non vedrei il bel <anchor xml:id="l6a3"/>viso<anchor xml:id="l6a4"/> amoroso.
            </l>
            <l xml:id="l7">
                Per cui piango, <anchor xml:id="l7a1"/>invidioso<anchor xml:id="l7a2"/>
            </l>
            <l xml:id="l8">
                di chi lʼha fatto <anchor xml:id="l8a1"/>suo<anchor xml:id="l8a2"/>, e me <anchor xml:id="l8a3"/>ne<anchor xml:id="l8a4"/> spoglia.
            </l>
        </lg>
      </body>
