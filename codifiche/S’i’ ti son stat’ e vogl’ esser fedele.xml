      <body>
         <lg>
            <l xml:id="l1"><anchor xml:id="l1a1"/>S'<anchor xml:id="l1a2"/>i’ ti son stat’ e vogl’ esser fedel<anchor xml:id="l1a3"/>e<anchor xml:id="l1a4"/></l>
            <l xml:id="l2"><anchor xml:id="l2a1"/>per<anchor xml:id="l2a2"/>ché non <anchor xml:id="l2a3"/>driç'<anchor xml:id="l2a4"/> <anchor xml:id="l2a5"/>a<anchor xml:id="l2a6"/> piatà le tuo vele?</l>
            </lg>
         <lg>
            <l xml:id="l3"><anchor xml:id="l3a1"/>Cruda<anchor xml:id="l3a2"/>, selvaggia <anchor xml:id="l3a3"/>donna, bell' e<anchor xml:id="l3a4"/> <anchor xml:id="l3a5"/>vaga<anchor xml:id="l3a6"/> </l>
            <l xml:id="l4"><anchor xml:id="l4a1"/>i'<anchor xml:id="l4a2"/> pur <anchor xml:id="l4a3"/>ti<anchor xml:id="l4a4"/> <anchor xml:id="l4a5"/>vincerò di<anchor xml:id="l4a6"/> <anchor xml:id="l4a7"/>lunga<anchor xml:id="l4a8"/> prova,</l>
            <l xml:id="l5" n="5">e non farà tua dureçça tal piaga</l>
            <l xml:id="l6"><anchor xml:id="l6a1"/>ch'a servir<anchor xml:id="l6a2"/> te mie fé non sia più nova,</l>
            <l xml:id="l7"><anchor xml:id="l7a1"/>la qual, cercando, altro <anchor xml:id="l7a3"/>modo<anchor xml:id="l7a4"/> non trova<anchor xml:id="l7a2"/></l>
            <l xml:id="l8">per farti ben piatosa di <anchor xml:id="l8a1"/>crudele<anchor xml:id="l8a2"/>.</l>
         </lg>
      </body>
