            <l xml:id="l1"><anchor xml:id="l1a1"/>Benché<anchor xml:id="l1a2"/> <anchor xml:id="l1a3"/>ora<anchor xml:id="l1a4"/> <anchor xml:id="l1a5"/>piova<anchor xml:id="l1a6"/>, <anchor xml:id="l1a7"/>pur buon tenp’<anchor xml:id="l1a8"/> <anchor xml:id="l1a9"/>aspecto<anchor xml:id="l1a10"/></l>
            <l xml:id="l2">al <anchor xml:id="l2a1"/>mie camin, et<anchor xml:id="l2a2"/> però <anchor xml:id="l2a3"/>non<anchor xml:id="l2a4"/> <anchor xml:id="l2a5"/>m’affretto<anchor xml:id="l2a6"/>.</l>
            </lg>
         <lg>
            <l xml:id="l3"><anchor xml:id="l3a1"/>Ogni<anchor xml:id="l3a2"/> <anchor xml:id="l3a3"/>cosa<anchor xml:id="l3a4"/> <anchor xml:id="l3a5"/>per ordin<anchor xml:id="l3a6"/> <anchor xml:id="l3a7"/>à suo tenpo<anchor xml:id="l3a8"/>;</l>      
