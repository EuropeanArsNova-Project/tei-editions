            <l xml:id="l5" n="5"><anchor xml:id="l5a1"/>Donna<anchor xml:id="l5a2"/> legiadra <anchor xml:id="l5a3"/>nel suo giovin tenpo<anchor xml:id="l5a4"/></l>
            <l xml:id="l6">agli ochi <anchor xml:id="l6a1"/>di ciasc<anchor xml:id="l6a2"/>un par graçïosa,</l>
            <l xml:id="l7"><anchor xml:id="l7a1"/>così vechieça la vede noiosa<anchor xml:id="l7a2"/></l>
            <l xml:id="l8"><anchor xml:id="l8a1"/>al<anchor xml:id="l8a2"/> <anchor xml:id="l8a3"/>guardo<anchor xml:id="l8a4"/> <anchor xml:id="l8a5"/>di<anchor xml:id="l8a6"/> chi più <anchor xml:id="l8a7"/>n’avea<anchor xml:id="l8a8"/> <anchor xml:id="l8a9"/>diletto<anchor xml:id="l8a10"/>.</l>
            </lg>
