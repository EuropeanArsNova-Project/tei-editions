      <body>
         <lg>
            <l xml:id="l1">Più bella <anchor xml:id="l1a1"/>donn' al<anchor xml:id="l1a2"/> mondo ma' non <anchor xml:id="l1a3"/>fia<anchor xml:id="l2a4"/></l>
            <l xml:id="l2">se 'n vo' regna <anchor xml:id="l2a1"/>pietat[e]<anchor xml:id="l2a2"/>,</l>
            <l xml:id="l3">ché ·lla <anchor xml:id="l3a1"/>biltat[e]<span type="space"/>adorn' e<anchor xml:id="l3a2"/> legiadria.</l>
            </lg>
         <lg>
            <l xml:id="l4">Gli <anchor xml:id="l4a1"/>ochi lucenti<anchor xml:id="l4a2"/> pien' di vaghi raçi,</l>
            <l xml:id="l5" n="5">gli <anchor xml:id="l5a1"/>ati<anchor xml:id="l5a2"/> legiadr' e' cape' <anchor xml:id="l5a3"/>biond' e<anchor xml:id="l5a4"/> folti,</l>
            <l xml:id="l6">che spesso sol per gli ochi so <anchor xml:id="l6a1"/>selvagi<anchor xml:id="l6a2"/>,</l>
            <l xml:id="l7">mi <anchor xml:id="l7a1"/>fan<anchor xml:id="l7a2"/> <anchor xml:id="l7a3"/>sparger<anchor xml:id="l7a4"/> gran pianti e sospir molti,</l>
            <l xml:id="l8">che dentro nella mente son racolti,</l>
            <l xml:id="l9">perch' io non vegio ancora</l>
            <l xml:id="l10" n="10"><anchor xml:id="l10a1"/>ve[n]ir<anchor xml:id="l10a2"/> quell’ora<span type="space"/>che siate pia.</l>
             </lg>
         <lg>
            <l xml:id="l11">Merçé, ahi lasso, i' sol merçé dimando</l>
            <l xml:id="l12">e <anchor xml:id="l12a1"/>b[e]n<anchor xml:id="l12a2"/> servendo degna pace <anchor xml:id="l12a3"/>aspet[t]o<anchor xml:id="l12a4"/>!</l>        
            <l xml:id="l13"><anchor xml:id="l13a1"/>Stringavi<anchor xml:id="l13a2"/> a mi piatate, oimé, mirando</l>
            <l xml:id="l14">gli gravi afanni del fedel sugetto,</l>
            <l xml:id="l15" n="15">e co·benigno e gratioso aspetto</l>
            <l xml:id="l16">d'alcun beato <anchor xml:id="l16a1"/>segno<anchor xml:id="l16a2"/></l>
            <l xml:id="l17">fatemi degno,<span type="space"/>com' e·cor disia.</l>
          </lg>
      </body>
